    <validationRules>
        <fullName>Cancellation_and_Downsell</fullName>
        <active>true</active>
        <description>Cancellations and Downsells must be Closed Realized, not Closed Lost.</description>
        <errorConditionFormula>(ISPICKVAL (Type, &quot;Cancellation&quot;) || ISPICKVAL (Type, &quot;Downsell&quot;))
&amp;&amp;
ISPICKVAL( StageName, &quot;Closed Lost&quot;)</errorConditionFormula>
        <errorDisplayField>StageName</errorDisplayField>
        <errorMessage>A Cancellation or Downsell cannot be Closed Lost, please choose Closed Realized.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Reopen_Closed_Lost_Opp</fullName>
        <active>true</active>
        <description>prevents users from re-opening a closed lost opp</description>
        <errorConditionFormula>ISCHANGED( StageName ) &amp;&amp; 
ISPICKVAL(PRIORVALUE (StageName), &quot;Closed Lost&quot;) &amp;&amp; 
NOT (  
$User.UserRoleId = &quot;00eE0000000te0a&quot;) /*Deployment Lead*/</errorConditionFormula>
        <errorMessage>You cannot reopen a Closed Lost Opp, please create a new opportunity or contact your Admin for assistance using the links in the sidebar</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Reopen_Closed_Won_Opp</fullName>
        <active>true</active>
        <description>prevents users from re-opening a closed won opp</description>
        <errorConditionFormula>ISCHANGED( StageName ) &amp;&amp; 
ISPICKVAL(PRIORVALUE (StageName), &quot;Closed Won&quot;) &amp;&amp; 
NOT ($User.Id = &quot;005E0000001sst8&quot;) /*Deployment Lead*/</errorConditionFormula>
        <errorMessage>You cannot reopen a Closed Won Opp, please create a new opportunity or contact your Admin for assistance using the links in the sidebar</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_move_Opp_back_to_Prospect</fullName>
        <active>true</active>
        <description>user cannot move stage back to Prospect</description>
        <errorConditionFormula>!ISPICKVAL( PRIORVALUE(StageName) , &quot;Prospect&quot;) &amp;&amp; ISPICKVAL(StageName, &quot;Prospect&quot;)</errorConditionFormula>
        <errorMessage>you cannot set the stage backwards to Prospecting - must update to Planning for Closed Lost</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Close_Date_Must_Be_a_Future_Date</fullName>
        <active>false</active>
        <description>checked when the opportunity is being created or the Close D</description>
        <errorConditionFormula>IF( IsClosed = False, CloseDate  &lt; TODAY(), CloseDate &gt; Today())</errorConditionFormula>
        <errorDisplayField>CloseDate</errorDisplayField>
        <errorMessage>Close Date cannot be a day in the past</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closed_Lost_must_have_a_Lost_Reason</fullName>
        <active>true</active>
        <description>If the Opp is Closed Lost, there must be a Lost Reason selected.</description>
        <errorConditionFormula>ISPICKVAL (StageName, &quot;Closed Lost&quot;)
&amp;&amp;
ISPICKVAL (Lost_Reason__c, &quot;&quot; )</errorConditionFormula>
        <errorDisplayField>StageName</errorDisplayField>
        <errorMessage>If the Opportunity is Closed Lost, you must select a Lost Reason.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Downsell_Must_be_Negative</fullName>
        <active>true</active>
        <description>Cancellation amount must be negative.</description>
        <errorConditionFormula>Amount &gt; 0 &amp;&amp; (ISPICKVAL(Type, &quot;Cancellation&quot;)  || ISPICKVAL(Type, &quot;Downsell&quot;))</errorConditionFormula>
        <errorDisplayField>Amount</errorDisplayField>
        <errorMessage>The Type of Opportunity is Cancellation or Downsell, so the Amount must be either zero or a negative number.  Please adjust the Sales Price to zero or a negative number.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MDC_can_only_create_opp_for_ATC_KBB_TIM</fullName>
        <active>true</active>
        <description>Prevents the MDC from creating Opportunities for vAuto, VinSolutions, and Homenet.</description>
        <errorConditionFormula>(
   $UserRole.Name = &quot;Media Dealer Consultant 10-1&quot;
|| $UserRole.Name = &quot;Media Dealer Consultant 10-2&quot;
|| $UserRole.Name = &quot;Media Dealer Consultant 10-3&quot;
|| $UserRole.Name = &quot;Media Dealer Consultant 10-4&quot;
|| $UserRole.Name = &quot;Media Dealer Consultant 19-1&quot;
|| $UserRole.Name = &quot;Media Dealer Consultant 19-2&quot;
|| $UserRole.Name = &quot;Media Dealer Consultant 19-3&quot;
|| $UserRole.Name = &quot;Media Dealer Consultant 19-4&quot;
)

&amp;&amp;

(  
   ISPICKVAL( BU__c , &quot;vAuto&quot;) 
|| ISPICKVAL( BU__c , &quot;Homenet&quot;) 
|| ISPICKVAL( BU__c , &quot;VinSolutions&quot;)
)</errorConditionFormula>
        <errorDisplayField>BU__c</errorDisplayField>
        <errorMessage>Media Dealer Consultants can only create Opportunities for the ATC Media, KBB, and TIM business units.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opportunity_Check_for_Products</fullName>
        <active>false</active>
        <description>Validates that an opportunity has opportunity products before the Stage can move beyond Prospect</description>
        <errorConditionFormula>OR(
   (AND
     (CASE(StageName, 
     &quot;Planning&quot;, 1, 
     &quot;Presentation&quot;, 1, 
     &quot;Closed Won&quot;, 1, 
     &quot;Closed Lost&quot;, 1, 
     &quot;Closed Realized&quot;, 1, 
     0) = 1, 
     NOT(HasOpportunityLineItem))), 
   (AND 
     (CASE(Type, 
     &quot;Renewal&quot;, 1, 
     0) = 1, 
     NOT(HasOpportunityLineItem))) 
)</errorConditionFormula>
        <errorMessage>STAGE field can only equal &quot;Prospect&quot; until a product has been added. TYPE field cannot equal &quot;Renewal&quot; until a product has been added.</errorMessage>
    </validationRules>
